  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dkshs/changelog-github": "^1.0.6",
    "@dkshs/prettier-config": "^1.0.0",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.2",
    "bundle-require": "^5.0.0",
    "eslint": "^9.5.0",
    "execa": "^9.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
