{
  "name": "@dkshs/eslint-config",
  "type": "module",
  "version": "2.10.2",
  "packageManager": "pnpm@9.9.0",
  "description": "DKSHS's ESLint config.",
  "license": "MIT",
  "homepage": "https://github.com/dkshs/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkshs/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/dkshs/eslint-config/issues"
  },
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm typegen && tsup",
    "dev": "pnpm dlx @eslint/config-inspector --config eslint-inspector.config.ts",
    "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "typegen": "tsx scripts/typegen.ts",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "changeset": "changeset",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/markdown": "^6.0.0",
    "@next/eslint-plugin-next": "14.3.0-canary.87",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.6",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-perfectionist": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-e56f4ae3-20240830",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.3.3",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.3.0",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@dkshs/changelog-github": "^1.0.6",
    "@dkshs/prettier-config": "^1.0.0",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.1",
    "bundle-require": "^5.0.0",
    "eslint": "^9.9.1",
    "eslint-typegen": "^0.3.1",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  },
  "prettier": "@dkshs/prettier-config"
}
